{
  "buildCommand": "cd client && npm install --legacy-peer-deps && CI=false GENERATE_SOURCEMAP=false npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
