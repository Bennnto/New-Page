{
  "version": 2,
  "name": "undercovered",
  "buildCommand": "cd client && npm install --legacy-peer-deps && CI=false GENERATE_SOURCEMAP=false npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install --legacy-peer-deps",
  "devCommand": "npm run dev",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024,
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
